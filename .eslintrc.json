{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "prefer-destructuring": ["error", { "object": true, "array": false }],
        "import/extensions": "off",
        "import/no-unresolved": "off"
    }
}
